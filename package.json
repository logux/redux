{
  "name": "@logux/redux",
  "version": "0.8.3",
  "description": "Redux compatible API for Logux",
  "keywords": [
    "logux",
    "client",
    "redux",
    "websocket"
  ],
  "scripts": {
    "unit": "tsm node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
    "test": "c8 pnpm unit && eslint . && check-dts && size-limit"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "homepage": "https://logux.io/",
  "repository": "logux/redux",
  "sideEffects": false,
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^12.0.0 || ^14.0.0 || >=16.0.0"
  },
  "peerDependencies": {
    "@logux/client": ">=0.10.0",
    "@logux/core": ">=0.7.0",
    "react": ">=16.8.4",
    "react-redux": ">=6.0.0",
    "redux": ">=4.0.0"
  },
  "dependencies": {
    "nanoevents": "^7.0.1"
  },
  "devDependencies": {
    "@logux/actions": "^0.2.3",
    "@logux/client": "^0.15.3",
    "@logux/core": "^0.7.3",
    "@logux/eslint-config": "^47.2.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/node": "^18.6.2",
    "@types/react": "^18.0.15",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "c8": "^7.12.0",
    "check-dts": "^0.6.7",
    "clean-publish": "^4.0.1",
    "eslint": "^8.20.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prefer-let": "^3.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "nanodelay": "^2.0.2",
    "nanospy": "^0.5.0",
    "nanostores": "^0.5.13",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-test-renderer": "^18.2.0",
    "redux": "^4.2.0",
    "size-limit": "^7.0.8",
    "tsm": "^2.2.2",
    "typescript": "^4.7.4",
    "uvu": "^0.5.6"
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "c8": {
    "exclude": [
      "**/*.test.*"
    ],
    "lines": 100,
    "check-coverage": true
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config/esm",
    "rules": {
      "@typescript-eslint/no-var-requires": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "no-console": "off"
    }
  },
  "eslintIgnore": [
    "**/errors.ts"
  ],
  "size-limit": [
    {
      "import": {
        "@logux/client": "{ CrossTabClient }",
        "./index.js": "{ createStoreCreator, useSubscription, useDispatch }"
      },
      "limit": "6 KB"
    }
  ]
}
